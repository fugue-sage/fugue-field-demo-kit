import Fugue.Core.AWS.Common as .
import Fugue.Core.AWS.EC2 as .
import Fugue.Core.AWS.SQS as .

type Topic:
  | Topic 
      name: String
      region: Region
      policy: Optional<String>
    

type Subscription:
  | Subscription 
      endpoint: EndpointType
      topic: Topic
    

type EndpointType:
  | Http String
  | Https String
  | Email String
  | EmailJson String
  | Sms Int
  | Sqs Queue
  | Application String
  | Lambda String

