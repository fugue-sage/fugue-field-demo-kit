
composition "FugueVpc"

#########################
# Import Network pattern
# and AWS common objects
#########################
import Fugue.AWS.Pattern.Network as Network
import Fugue.Core.AWS.Common as AWS

#########################
# Declare Network (VPC)
#########################
demo-app-network: Network.new {
  name: "demo-app-network",
  region: AWS.Us-west-2,
  cidr: "10.0.0.0/16",
  publicSubnets: [
    (AWS.A, "10.0.1.0/24"),
    (AWS.B, "10.0.2.0/24"),
  ],
  privateSubnets: [],
}
